{
    "name": "zfbrasil/doctrine-money-module",
    "description": "Provide ZF integration between doctrine and mathiasverraes/money",
    "license": "MIT",
    "authors": [
        {
            "name": "fabiocarneiro",
            "email": "fahecs@gmail.com"
        },
        {
            "name": "Gabriel Schmitt",
            "email": "gabrielsch@outlook.com"
        },
        {
            "name": "Daniel Gimenes",
            "email": "daniel@danielgimenes.com.br"
        }
    ],
    "require-dev": {
    	"phpunit/phpunit": "^4.4",
        "friendsofphp/php-cs-fixer": "^1.11",
        "doctrine/dbal": "^2.5",
        "doctrine/mongodb-odm": "^1.0"
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "doctrine/doctrine-module": "^0.9 || ^1.2",
        "zendframework/zend-form": "^2.3",
        "zendframework/zend-filter": "^2.3",
        "zendframework/zend-validator": "^2.3",
        "zendframework/zend-view": "^2.3",
        "zendframework/zend-servicemanager": "^2.3 || ^3.3",
        "zendframework/zend-modulemanager": "^2.3",
        "zendframework/zend-i18n": "^2.3",
        "mathiasverraes/money": "^1.2"
    },
    "suggest": {
        "doctrine/dbal": "^2.5",
        "doctrine/mongodb-odm": "^1.0",
        "doctrine/orm": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "ZFBrasil\\DoctrineMoneyModule\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZFBrasil\\Test\\DoctrineMoneyModule\\" : "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
